<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".launch_mode_demo.ThirdActivity">

    <Button
        android:id="@+id/launch_second_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/launch_second_activity"
        />

    <Button
        android:id="@+id/launch_fourth_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/launch_fourth_activity"
        />

</LinearLayout>